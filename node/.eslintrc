{
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": 0,
        "quotes": [2, "single"],
        "indent": [2, 4],
        "comma-spacing": 2,
        "comma-style": 2,
        "brace-style": 2,
        "space-before-blocks": 2,
        "keyword-spacing": 2,
        "key-spacing": 2,
        "spaced-comment": 2,
        "space-infix-ops": 2,
        "comma-dangle": 2
    }
}
